{
  "name": "mf-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "npm run start:shell",
    "start2": "npm run start:detail-user",
    "start:shell": "ng serve shell  -o --port 5000",
    "start:detail-user": "ng serve detail-user -o --port 3000",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:detail-user\" ",
    "build:shell": "ng build shell --prod",
    "build:detail-user": "ng build detail-user --prod",
    "serve:dist": "concurrently \"serve dist/shell -l 5000 -s\" \"serve dist/detail-user -l 3000 -s\" ",
    "build": "npm run build:shell && npm run build:detail-user",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.3.0",
    "@angular-architects/module-federation-tools": "^14.3.0",
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "yarn": "^1.22.19",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
